<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!--        __________________report_close_the_box___________________         -->
<!--    XLS  -->
    <record id="action_close_box_xls" model="ir.actions.report">
        <field name="name">Reporte cierre de caja XLS</field>
        <field name="model">wizard.close.box</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">advanced_reports.report_close_box_xls</field>
        <field name="report_file">advanced_reports.report_close_box_xls</field>
        <field name="binding_model_id" eval="False"/>
    </record>

    <record id="action_close_box_commission_xls" model="ir.actions.report">
        <field name="name">Reporte cierre de caja para comisi√≥n XLS</field>
        <field name="model">wizard.close.box.commission</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">advanced_reports.report_close_box_commission_xls</field>
        <field name="report_file">advanced_reports.report_close_box_commission_xls</field>
        <field name="binding_model_id" eval="False"/>
    </record>
<!--    PDF   -->
    <record id="close_box_pdf_format" model="report.paperformat">
        <field name="name">Cierre de caja PDF</field>
        <field name="default" eval="True"/>
        <field name="format">Letter</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">5</field>
        <field name="margin_bottom">30</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">35</field>
        <field name="dpi">90</field>

    </record>


    <record id="action_close_box_pdf" model="ir.actions.report">
        <field name="name">Reporte cierre de caja PDF</field>
        <field name="model">wizard.close.box</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">advanced_reports.report_close_box</field>
        <field name="report_file">advanced_reports.report_close_box</field>
        <field name="paperformat_id" ref="close_box_pdf_format"/>
        <field name="binding_model_id" eval="False"/>
    </record>
