<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
        <record id="view_account_journal_form" model="ir.ui.view">
            <field name="name">account.journal.form.inherit</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <field name="type" position="after">
                    <field name="code" invisible="1"/>
                    <field name="sv_fe_active" widget="boolean_toggle" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_country_code != 'SV'" />
                    <field name="sv_fe_type" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_country_code != 'SV'" />
                    <field name="sv_fe_establishment_id" domain="[('company_id', '=', company_id)]" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_country_code != 'SV'" />
                </field>
            </field>
        </record>

    </data>
</odoo>
