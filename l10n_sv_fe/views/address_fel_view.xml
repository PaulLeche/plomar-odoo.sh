<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="code_activity_form_view" model="ir.ui.view">
            <field name="name">code_activity_fel.code</field>
            <field name="model">codigoactividad.fel</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="activity_name"/>
                        <field name="code"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="code_activity_tree_view" model="ir.ui.view">
            <field name="name">code_activity_tree_view</field>
            <field name="model">codigoactividad.fel</field>
            <field name="arch" type="xml">
                <list string="Actividades Económicas">
                    <field name="activity_name"/>
                    <field name="code"/>
                </list>
            </field>
        </record>

        <record id="code_activity_fel_act_window" model="ir.actions.act_window">
            <field name="name">code_activity</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">codigoactividad.fel</field>
            <field name="view_mode">form</field>
            <field name="view_mode">list,form</field>
        </record>

        <menuitem
            action="code_activity_fel_act_window" id="menu_code_activity_fel_form_action"
            name="Actividades Económicas FEL"
            parent="account.menu_finance_configuration" sequence="5"/>

    </data>
</odoo>