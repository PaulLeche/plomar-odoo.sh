<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_template_type_view" model="ir.ui.view">
            <field name="name">product.type.view.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='group_general']" position="inside">
                    <field name="sv_fe_services" string="Tipo Servicio FEL" force_save="1" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_product_country_code != 'SV'" />
                    <field name="sv_fe_unidad_medida_id" string="Unidad de medida FEL" force_save="1" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_product_country_code != 'SV'" />
                    <field name="sv_fe_tributes_id" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_product_country_code != 'SV'" />
                </xpath>
            </field>
        </record>

        <record id="product_product_type_view" model="ir.ui.view">
            <field name="name">product.product.type.view.inherit</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='barcode']" position="after">
                    <field name="sv_fe_services" string="Tipo Servicio FEL" force_save="1" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_product_country_code != 'SV'" />
                    <field name="sv_fe_unidad_medida_id" string="Unidad de medida FEL" force_save="1" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_product_country_code != 'SV'" />
                    <field name="sv_fe_tributes_id" groups="l10n_sv_fe.sv_fe_group_fel_manager" invisible="sv_fe_product_country_code != 'SV'" />
                </xpath>
            </field>
        </record>

    </data>
</odoo>