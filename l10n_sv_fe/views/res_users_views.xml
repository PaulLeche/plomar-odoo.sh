<?xml version="1.0" encoding="utf-8"?>
<odoo>

  	<record id="base_view_users_form_inherit" model="ir.ui.view">
		<field name="name">base.view.users.form.inherit</field>
		<field name="model">res.users</field>
		<field name="priority">5</field>
		<field name="inherit_id" ref="base.view_users_form"/>
		<field name="arch" type="xml">
			<xpath expr="//page[@name='preferences']" position="inside">
                <group string="Establecimiento" groups="l10n_sv_fe.group_fel_manager">
                    <field name="fe_establishment_id"/>
					<field name="identification_type"/>
					<field name="vat" string="NIT" invisible="identification_type != '36'"/>
				 	<field name="dui_field" invisible="identification_type != '13'"/>
				 	<field name="other_field" invisible="identification_type != '37'"/>
				 	<field name="passport_field" invisible="identification_type != '03'"/>
				 	<field name="carnet_residente_field" invisible="identification_type != '02'"/>
                </group>
			</xpath>
		</field>
	</record>

</odoo>