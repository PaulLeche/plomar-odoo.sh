<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_sv_report" model="ir.ui.view">
        <field name="name">view.move.form.sv.report</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@id='other_tab']/group[@id='other_tab_group']" position="inside">
                <group string="FacturaciÃ³n" invisible="move_type != 'in_invoice'">
                    <field name="class_of_document" readonly="1"/>
                    <field name="number_of_series_document"/>
                </group>

                <group string="Orden de Compra" invisible="move_type != 'in_invoice'">
                    <field name="document_classification"/>
                    <field name="sector"/>
                    <field name="type_of_cost_or_expense"/>
                    <field name="type_of_operation"/>
                </group>
           </xpath>
            <xpath expr="//notebook/page[@id='invoice_tab']/field/tree/field[@name='name']" position="after">
                <field name="type_of_purchase" optional="hide"/>
            </xpath>

            <field name="numero_control" position="after">
                <field name="class_of_document" readonly="1"/>
                <field name="number_internal_control"/>
                <field name="number_of_resolution"/>
                <field name="number_of_series_document"/>
            </field>

            <xpath expr="//notebook/page[@id='other_tab']/group[@id='other_tab_group']/group" position="inside">
                <field name="date_of_issue_voucher_of_liquidation" groups="base.group_no_one"/>
                <field name="number_of_issue_voucher_of_liquidation" groups="base.group_no_one"/>
                <field name="resolution_of_voucher_of_liquidation" groups="base.group_no_one"/>
                <field name="series_of_voucher_of_liquidation" groups="base.group_no_one"/>
            </xpath>

            <xpath expr="//notebook/page[@id='other_tab']/group[@id='other_tab_group']/group[@name='accounting_info_group']" position="inside">
                <field name="number_of_annexed_1" groups="base.group_no_one" invisible="move_type != 'out_invoice'" readonly="1"/>
                <field name="number_of_annexed_2" groups="base.group_no_one" invisible="move_type != 'out_invoice'" readonly="1"/>
                <field name="number_of_annexed_4" groups="base.group_no_one" invisible="move_type != 'out_invoice'" readonly="1"/>
                <field name="number_of_annexed_7" groups="base.group_no_one" invisible="move_type != 'out_invoice'" readonly="1"/>
                <field name="number_of_annexed_10" groups="base.group_no_one" invisible="move_type != 'out_invoice'" readonly="1"/>
                <field name="number_of_annexed_12" groups="base.group_no_one" invisible="move_type != 'out_invoice'" readonly="1"/>

                <field name="number_of_annexed_3" groups="base.group_no_one" invisible="move_type != 'in_invoice'" readonly="1"/>
                <field name="number_of_annexed_5" groups="base.group_no_one" invisible="move_type != 'in_invoice'" readonly="1"/>
                <field name="number_of_annexed_6" groups="base.group_no_one" invisible="move_type != 'in_invoice'" readonly="1"/>
                <field name="number_of_annexed_8" groups="base.group_no_one" invisible="move_type != 'in_invoice'" readonly="1"/>
                <field name="number_of_annexed_9" groups="base.group_no_one" invisible="move_type != 'in_invoice'" readonly="1"/>
                <field name="number_of_annexed_11" groups="base.group_no_one" invisible="move_type != 'in_invoice'" readonly="1"/>
            </xpath>

        </field>
    </record>

