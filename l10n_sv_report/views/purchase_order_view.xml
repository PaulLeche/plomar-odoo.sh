<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="purchase_order_form_sv" model="ir.ui.view">
        <field name="name">purchase.order.form.sv</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            <field name="currency_id" position="after">
                <field name="document_classification"/>
                <field name="sector"/>
                <field name="type_of_cost_or_expense"/>
                <field name="type_of_operation"/>
            </field>

            <xpath expr="//notebook/page[@name='products']/field/list/field[@name='name']" position="after">
                <field name="type_of_purchase"/>
            </xpath>

        </field>
    </record>

</odoo>