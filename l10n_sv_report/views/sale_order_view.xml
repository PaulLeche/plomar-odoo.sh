<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_order_form_inherit_delivery_ups" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.delivery.ups</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='order_lines']/field/list/field[@name='name']" position="after">
                <field name="type_of_sale"/>
            </xpath>
        </field>
    </record>
</odoo>