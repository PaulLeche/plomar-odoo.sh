<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	
		<record id="account_invoice_report_form" model="ir.ui.view">
			<field name="name">account.move.form.inherit1</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='ref']" position="replace">
					<field name="ref" invisible="1"/>
				</xpath>
				<xpath expr="//field[@name='invoice_line_ids']/list/field[@name='quantity']" position="before">
					<field name="tipo_gasto" force_save="1" optional="show"/>
				</xpath>
				<xpath expr="//field[@name='line_ids']/list/field[@name='name']" position="after">
					<field name="tipo_gasto" force_save="1" invisible="1"/>
				</xpath>
			</field>
		</record>

	</data>
</odoo>